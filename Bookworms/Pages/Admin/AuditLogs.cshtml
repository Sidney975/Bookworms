@page
@model Bookworms.Pages.Admin.AuditLogsModel
@{
    ViewData["Title"] = "Audit Logs";
}

<h2>Audit Logs</h2>

<table class="table">
    <thead>
        <tr>
            <th>User ID</th>
            <th>Action</th>
            <th>Description</th>
            <th>Timestamp</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model.Logs)
        {
            <tr>
                <td>@log.UserId</td>
                <td>@log.Action</td>
                <td>@log.Description</td>
                <td>@log.Timestamp</td>
            </tr>
        }
    </tbody>
</table>
